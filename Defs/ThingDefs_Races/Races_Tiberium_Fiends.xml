<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<ThingDef ParentName="AnimalThingBase">
		<defName>TiberiumThrumbo</defName>
		<label>Thrimbo</label>
		<description>A gigantic creature of unknown origin. The Thrumbo, but this one seems to have an Tiberium infection. Be careful, it might be easily enraged.</description>
		<statBases>
			<MoveSpeed>5.25</MoveSpeed>
			<ComfyTemperatureMin>-40</ComfyTemperatureMin>
			<MarketValue>6400</MarketValue>
			<MeatAmount>180</MeatAmount>
			<!-- more than implied by body size -->
		</statBases>
		<verbs>
			<li>
				<verbClass>Verb_MeleeAttack</verbClass>
				<defaultCooldownTime>1.90</defaultCooldownTime>
				<meleeDamageBaseAmount>32</meleeDamageBaseAmount>
				<meleeDamageDef>Scratch</meleeDamageDef>
				<linkedBodyPartsGroup>HornAttackTool</linkedBodyPartsGroup>
			</li>
		</verbs>
		<race>
			<body>QuadrupedAnimalWithHoovesAndHorn</body>
			<deathActionWorkerClass>DeathActionWorker_DropBodyParts</deathActionWorkerClass>
			<herdAnimal>true</herdAnimal>
			<baseBodySize>3.5</baseBodySize>
			<baseHungerRate>4.5</baseHungerRate>
			<baseHealthScale>13.0</baseHealthScale>
			<foodType>VegetarianRoughAnimal, DendrovoreAnimal</foodType>
			<leatherColor>(233,233,233)</leatherColor>
			<leatherInsulation>1.35</leatherInsulation>
			<leatherCommonalityFactor>0.1</leatherCommonalityFactor>
			<leatherMarketValueFactor>7</leatherMarketValueFactor>
			<leatherLabel>thrimbofur</leatherLabel>
			<leatherStatFactors>
				<ArmorRating_Blunt>1.5</ArmorRating_Blunt>
				<ArmorRating_Sharp>6.0</ArmorRating_Sharp>
				<ArmorRating_Heat>7.4</ArmorRating_Heat>
			</leatherStatFactors>
			<trainableIntelligence>Advanced</trainableIntelligence>
			<gestationPeriodDays>80</gestationPeriodDays>
			<manhunterOnDamageChance>1</manhunterOnDamageChance>
			<manhunterOnTameFailChance>0.10</manhunterOnTameFailChance>
			<wildness>0.985</wildness>
			<lifeExpectancy>130</lifeExpectancy>
			<lifeStageAges>
				<li>
					<def>AnimalBaby</def>
					<minAge>0</minAge>
					</li>
				<li>
					<def>AnimalJuvenile</def>
					<minAge>6.0</minAge>
				</li>
				<li>
					<def>AnimalAdult</def>
					<minAge>25.0</minAge>
					<soundCall>Pawn_Thrumbo_Call</soundCall>
					<soundAngry>Pawn_Thrumbo_Angry</soundAngry>
					<soundWounded>Pawn_Thrumbo_Pain</soundWounded>
					<soundDeath>Pawn_Thrumbo_Death</soundDeath>
				</li>
			</lifeStageAges>
			<soundMeleeHitPawn>Pawn_Melee_BigBash_HitPawn</soundMeleeHitPawn>
			<soundMeleeHitBuilding>Pawn_Melee_BigBash_HitBuilding</soundMeleeHitBuilding>
			<soundMeleeMiss>Pawn_Melee_BigBash_Miss</soundMeleeMiss>
		</race>
	</ThingDef>

	<PawnKindDef ParentName="AnimalKindBase">
		<defName>Thrimbo</defName>
		<label>thrimbo</label>
		<race>Thrumbo</race>
		<combatPower>600</combatPower>
		<wildSpawn_spawnWild>false</wildSpawn_spawnWild>
		<lifeStages>
			<li>
				<label>tibro calf</label>
				<labelPlural>tibro calves</labelPlural>
				<bodyGraphicData>
					<texPath>Pawn/Animals/Thrimbo/Thrimbo</texPath>
					<drawSize>2.5</drawSize>
					<shadowData>
						<volume>(0.4, 0.3, 0.3)</volume>
						<offset>(0,0,-0.2)</offset>
					</shadowData>
				</bodyGraphicData>
				<dessicatedBodyGraphicData>
					<texPath>Things/Pawn/Animal/Dessicated/CritterDessicatedMedium</texPath>
					<drawSize>1.5</drawSize>
				</dessicatedBodyGraphicData>
			</li>
			<li>
				<bodyGraphicData>
					<texPath>Pawn/Animals/Thrimbo/Thrimbo</texPath>
					<drawSize>4.0</drawSize>
					<shadowData>
						<volume>(0.7, 0.6, 0.4)</volume>
						<offset>(0,0,-0.25)</offset>
					</shadowData>
				</bodyGraphicData>
				<dessicatedBodyGraphicData>
					<texPath>Things/Pawn/Animal/Dessicated/CritterDessicatedMedium</texPath>
					<drawSize>1.75</drawSize>
				</dessicatedBodyGraphicData>
			</li>
			<li>
				<bodyGraphicData>
					<texPath>Pawn/Animals/Thrimbo/Thrimbo</texPath>
					<drawSize>4.8</drawSize>
					<shadowData>
						<volume>(1, 0.8, 0.5)</volume>
						<offset>(0,0,-0.5)</offset>
